<resources>
    <attr name="alertDialogTitleDividerColor" format="reference|color" />
    <attr name="buttonBarStyle" format="reference" />
    <attr name="dividerHorizontal" format="reference" />
    <attr name="buttonBarButtonStyle" format="reference" />

    <attr name="spinnerDropDownItemStyle_apptheme" format="reference" />


    <attr name="tabBackground" format="reference" />
    <attr name="pda_background" format="reference" />
    <attr name="textThemeItemIsnew" format="reference" />
    <attr name="defaultBackground" format="reference" />
    <attr name="defaultTextColor" format="reference" />
    <attr name="defaultColors" format="reference" />
    <attr name="forumItemSelector" format="reference" />
    <attr name="categoryName" format="reference" />
    <attr name="boardForumName" format="reference" />
    <attr name="devdb_group" format="reference" />
    <attr name="devdb_prop" format="reference" />
    <attr name="devdb_value" format="reference" />

    <declare-styleable name="ExpandoLayout">
        <attr name="text" format="string" />
        <attr name="titleRowPadding" format="integer" />
        <attr name="expanded" format="boolean" />
        <attr name="moreBar" format="boolean" />
        <attr name="moreBarDefaultViewHeight" format="integer" />
        <attr name="moreBarFadeColor" format="color" />
        <attr name="moreBarOnRight" format="boolean" />
        <attr name="compactText" format="string" />
        <attr name="expandedText" format="string" />
        <attr name="showAndHideChildren" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ExpandablePanel">
        <attr name="handle" format="reference" />
        <attr name="content" format="reference" />
        <attr name="collapsedHeight" format="dimension" />
        <attr name="animationDuration" format="integer" />
    </declare-styleable>

    <declare-styleable name="ExtEditTextPreference">
        <attr name="myInputType" format="enum">
            <enum name="number" value="0" />
            <enum name="numberDecimal" value="1" />
        </attr>
        <attr name="defaultValue" format="string" />

    </declare-styleable>

    <declare-styleable name="SearchSettingsView">
        <attr name="user_name" format="string" />
        <attr name="source" format="enum">
            <enum name="all" value="0"/>
            <enum name="top" value="1"/>
            <enum name="pst" value="2"/>
        </attr>
    </declare-styleable>
</resources>